});

function topLeft() {
	$('#main_image').css('-webkit-transform-origin','1px 1px');
}

function zoomIn() {
	$('#main_image').css('-webkit-transform','scale(1.13)');
}

function zoomOut() {
	$('#main_image').css('-webkit-transform','scale(1.0)');
}


</script>

